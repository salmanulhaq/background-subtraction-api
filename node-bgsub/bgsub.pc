Libs: -lopencv_video -lopencv_objdetect -lopencv_highgui.so -lopencv_imgproc.so -lopencv_core.so /usr/lib/x86_64-linux-gnu/libcurl.so

Cflags: -I${includedir_old} -I${includedir_new} -I${inc} -I${local_inc}
